import java.text.DecimalFormat;

public class one 
{
	public static void main(String[] args)
	{

	}
	
	public static double distance(int x1, int y1, int x2, int y2) //Author: Martin Vera 
	{
		int x = x2 - x1;
		x = (int) Math.pow(x, 2);
		
		int y = y2 - y1;
		y = (int) Math.pow(y, 2);
		
		return Math.sqrt(x + y);
		
	}
}
